{
  "markdownlint.config": {
    "default": true,
    "MD007": { "indent": 4 }
  },
  "markdown.extension.list.indentationSize": "inherit",
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
  },
  "cSpell.words": [
    "ASTERES",
    "autohide",
    "cheatsheet",
    "fontawesome",
    "javascripts",
    "linebreak",
    "magiclink",
    "materialx",
    "mkdocs",
    "Nemah",
    "pymdownx",
    "scottgigawatt",
    "superfences",
    "tablesort",
    "tasklist",
    "twemoji",
    "venv"
  ],
}
